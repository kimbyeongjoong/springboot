{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "ignorePatterns": [
        "/src/main/resources/static/js/babel/*.js"
    ],
    "rules": {
        "no-var": "warn",
        "no-unused-vars": "warn",
        "prefer-const": "off",
        "prettier/prettier": [
            "warn",
            {
                "endOfLine": "auto"
            }
        ]
    }
}
